<%= simple_form_for(@project) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :title %>
    <%= f.input :subtitle %>
    <%= f.input :des %>
    <%= f.input :s1 %>
    <%= f.input :s2 %>
    <%= f.input :s3 %>
    <%= f.input :s4 %>
    <%= f.input :s5 %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
