<div class"container">
    <div class="prof-jumbotron">
         
    </div>

  </div>
  

    <div class="container">


    <div class="prof-photo">
                    
                        <%= image_tag @user.avatar.url(:small) %>
                    
                    
    </div>
    <div class="container">
        <div class="name-container">
          <div class="user-name">Hi, I'm <%=current_user.fullname%><br><small></small></div>

      </div>
      <div class="skills-container">
                        <span><strong>Skills: </strong></span>
                        <span class="label label-warning">HTML5</span>
                        <span class="label label-warning">CSS</span>
                        <span class="label label-info">Adobe CC</span>
                        <span class="label label-info">Google</span>
                        <span class="label label-success">OSX</span>
                        <span class="label label-success">iOS</span>
                        <span class="label label-warning">JS</span>
                        <span class="label label-warning">Keynote</span>
                        <span class="label label-info">Entreprenuership</span>
                        

      </div>



     
    


          <div class="row">

            <div class="project-card">
                <h3 class="page-header">What I'm Working On:</h3>
            </div>
            <%if @projects %>
          <%@projects.each do |project|%>
          <div class="container">
          <div class="row">
          <div class="col-md-4">
          <div class="thumbnail">
            <img src="http://placehold.it/800x500" alt="">
          <h2><%=link_to (project.title), project_path(project)%></h2>
          <p><%=project.des%></p>

          <p><a class="btn btn-default" href="#" role="button">Explore &raquo;</a></p>
          </div>
       </div>
        <%end%>
        <%end%>

    
      <hr>
      <footer>
